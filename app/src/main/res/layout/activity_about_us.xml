<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:padding="25dp"
        android:layout_height="match_parent">
<RelativeLayout
        android:layout_width="match_parent"
        android:padding="25dp"
        android:id="@+id/rl1"
        android:layout_height="wrap_content">
    <de.hdodenhof.circleimageview.CircleImageView
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/mpic"
             android:id="@+id/pic1"/>
    <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/pic1"
            android:text="AYUSH AGARWAL ID:2018kucp1009"
            android:textSize="25dp"
            android:layout_marginLeft="15dp"
    />
</RelativeLayout>
    <RelativeLayout
            android:layout_width="match_parent"
            android:padding="25dp"
            android:id="@+id/rl2"
            android:layout_below="@id/rl1"
            android:layout_height="wrap_content">
        <de.hdodenhof.circleimageview.CircleImageView
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/prnpic"
                android:id="@+id/pic2"/>
        <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/pic2"
                android:text="Pranav Sindura   ID:2018kucp1020"
                android:textSize="25dp"
                android:layout_marginLeft="15dp"
        /></RelativeLayout>
        <RelativeLayout
                android:layout_width="match_parent"
                android:padding="25dp"
                android:id="@+id/rl3"
                android:layout_below="@id/rl2"
                android:layout_height="wrap_content">
            <de.hdodenhof.circleimageview.CircleImageView
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/tpic"
                    android:id="@+id/pic3"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/pic3"
                    android:text="Tanmay Gemini ID:2018kucp1011"
                    android:textSize="25dp"
                    android:layout_marginLeft="15dp"
            /></RelativeLayout>
            <RelativeLayout
                    android:layout_width="match_parent"
                    android:padding="25dp"
                    android:id="@+id/rl4"
                    android:layout_below="@id/rl3"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:text="Pravash , Bhavanshu"
                        android:textSize="25dp"
                        android:layout_marginLeft="15dp"
                /></RelativeLayout>
                <RelativeLayout
                        android:layout_width="match_parent"
                        android:padding="25dp"
                        android:id="@+id/rl5"
                        android:layout_below="@id/rl4"
                        android:layout_height="wrap_content">
                    <de.hdodenhof.circleimageview.CircleImageView
                            android:layout_width="60dp"
                            android:layout_height="60dp"
                            android:id="@+id/pic5"/>
                    <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_toRightOf="@id/pic5"
                            android:text="AYUSH AGARWAL ID:2018kucp1009"
                            android:textSize="25dp"
                            android:layout_marginLeft="15dp"
                    />
                </RelativeLayout>
</RelativeLayout>